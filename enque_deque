a#include<stdio.h>
#include<stdlib.h>
#define MAX 5
int top=-1, STK[MAX];
void enque()
{
	int ele;
	if(top==MAX-1)
	printf("stack is full:");
	else
	{
		printf("Enter the ele:");
		scanf("%d",&ele);
	top=top+1;
	STK[top]=ele;	
	}
}
void deque()
{
	if(top==-1)
	printf("stack is empty");
	else
	top=top-1;
}
void create()
{
	if(top==-1)
	printf("stack is empty");
	else
	printf("the top element=%d",STK[top]);
}
void display()
{
	int i;
	if(top==-1)
	printf("stack is empty");
	else
	for(i=top;i>=0;i--)
	printf("%d\n",STK[i]);
}
void insert_at_end()
{
	int ele;
	printf("enter the v")
}
int main()
{
	int cho;
	do{
		printf("\n main menu\n");
		printf("\n 1.enque");
		printf("\n 2.deque");
		printf("\n 3.create");
		printf("\n 4.inser_at_begin");
		printf("\n 5.insert_at_end");
		printf("\n Enter your choice");
		scanf("%d",&cho);
		switch(cho)
		{
			case 1:enque();break;
			case 2:deque();break;
			case 3:create();break;
			case 4:insert_at_begin();break;
			case 5:insert_at_end();break;
			default:printf("Enter the choice between 1 to 5");
		}
	}while(cho>=1 && cho<=5);
	return 0;
}
